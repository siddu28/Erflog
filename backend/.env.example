# =============================================================================
# Career Flow AI - Environment Variables Example
# Copy this file to .env and fill in your values
# =============================================================================

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_JWT_SECRET=your-jwt-secret

# Google AI (Gemini) API Key
GOOGLE_API_KEY=your-google-api-key
# Or use GEMINI_API_KEY (alias)
# GEMINI_API_KEY=your-gemini-api-key

# =============================================================================
# Interview Configuration - Technical
# =============================================================================
INTERVIEW_TECHNICAL_TOTAL_TURNS=6
INTERVIEW_TECHNICAL_INTRO_TURNS=1
INTERVIEW_TECHNICAL_RESUME_TURNS=2
INTERVIEW_TECHNICAL_CHALLENGE_TURNS=2
INTERVIEW_TECHNICAL_CONCLUSION_TURNS=1

# =============================================================================
# Interview Configuration - HR / Behavioral
# =============================================================================
INTERVIEW_HR_TOTAL_TURNS=6
INTERVIEW_HR_INTRO_TURNS=1
INTERVIEW_HR_BEHAVIORAL_TURNS=2
INTERVIEW_HR_EXPERIENCE_TURNS=2
INTERVIEW_HR_CONCLUSION_TURNS=1

# =============================================================================
# Audio Configuration (Voice Interview)
# =============================================================================
AUDIO_SILENCE_THRESHOLD=500
AUDIO_SILENCE_DURATION=0.8
AUDIO_COOLDOWN_SECONDS=1.0

# =============================================================================
# Server Configuration
# =============================================================================
PORT=8000
